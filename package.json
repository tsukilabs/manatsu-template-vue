{
  "name": "manatsu-template-vue",
  "version": "0.5.1",
  "description": "Vue template for Manatsu",
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json,yaml}": "prettier --write",
    "*.{?(c|m)@(j|t)s,vue}": "pnpm lint"
  },
  "scripts": {
    "build": "pnpm run type-check && vite build",
    "dev": "vite",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "prepare": "husky",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "manatsu": "^0.4.15",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^2.1.2",
    "@tb-dev/utility-types": "^1.2.0",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-vue-devtools": "^7.0.16",
    "vue-tsc": "^2.0.4"
  }
}

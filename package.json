{
  "name": "manatsu-template-vue",
  "version": "0.4.2",
  "description": "Vue template for Manatsu",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json,yaml}": "prettier --write",
    "*.{?(c|m)@(j|t)s,vue}": "pnpm lint"
  },
  "scripts": {
    "build": "pnpm run type-check && vite build",
    "dev": "vite",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "prepare": "husky install",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "manatsu": "^0.4.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^1.6.7",
    "@tb-dev/utility-types": "^1.1.0",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "taze": "^0.13.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-vue-devtools": "^7.0.13",
    "vue-tsc": "^1.8.27"
  }
}
